{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "customer",
    "type": "object",
    "properties": {
        "username": 
        {
        "type": "string",
        "minLength": 1,
        "maxLength": 32
        },
        "email": 
        {
        "type": "string",
        "pattern": "^\\S+@\\S+\\.\\S+$",
        "minLength": 5,
        "maxLength": 50
        },
        "phonenumber": 
        {
            "type": "string",
            "pattern": "^\\d{10}$",
            "minLength": 8,
            "maxLength": 32
        },
        "password": 
        {
        "type": "string",
        "minLength": 8,
        "maxLength": 32
        },
        "imageID": 
        {
        "type": "integer",
        "minimum": 1
        }
    },
    "required": [
    "username",
    "email",
    "phonenumber",
    "password",
    "imageID"
    ]
}